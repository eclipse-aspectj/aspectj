<!DOCTYPE aspectj PUBLIC "-//AspectJ//DTD//EN" "http://sswww.aspectj.org/dtd/aspectj_1_5_0.dtd">
<aspectj>
    <weaver options="-showWeaveInfo">
        <include within="foo..bar.Goo+"/>
        <dump within="foo..*" beforeandafter="true"/>
    </weaver>
