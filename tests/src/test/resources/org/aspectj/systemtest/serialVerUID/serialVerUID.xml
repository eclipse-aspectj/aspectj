        <run class="Test"/>
		<compile files="Test.java, Util.java, AroundClosureExecutionAdvice.aj"
		  xlintfile="Xlint.properties">
            <message kind="warning" line="9"/>
		</compile>
        <run class="Util" options="-fail"/>
	</ajc-test>

