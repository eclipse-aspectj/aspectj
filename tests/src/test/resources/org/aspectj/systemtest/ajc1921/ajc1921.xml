			<stdout>
				<line text="FirstAspect"/>
				<line text="SecondAspect"/>
				<line text="Executing task"/>
				<line text="FirstAspect"/>
				<line text="SecondAspect"/>
				<line text="Executing task"/>
				<line text="FirstAspect"/>
				<line text="SecondAspect"/>
				<line text="Executing task"/>
				<line text="FirstAspect"/>
				<line text="SecondAspect"/>
				<line text="Executing task"/>
				<line text="FirstAspect"/>
				<line text="SecondAspect"/>
				<line text="Executing task"/>
				<line text="Finished executing tasks"/>
